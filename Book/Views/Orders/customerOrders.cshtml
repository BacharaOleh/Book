            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.bookId)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.quantity)
            </td>
