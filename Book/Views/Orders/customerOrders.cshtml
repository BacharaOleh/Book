@model IEnumerable<Book.Models.Orders>

@{
    ViewData["Title"] = "customerOrders";
}

<h1>customerOrders</h1>
<body style="background-color: lightgreen;">

</body>
<p>
    <a asp-action="Create">Utworż nowy</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.bookId)
            </th>
           
